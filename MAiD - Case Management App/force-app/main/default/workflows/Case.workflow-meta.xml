    <rules>
        <fullName>MAiD Death All forms Recieved</fullName>
        <actions>
            <name>Status_Changes_to_Ready_for_CC_Review</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>equals</operation>
            <value>MAiD Death Foreseeable,MAiD Death Not Foreseeable</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.X1632_Date_Received__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.X1633_Date_Received__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.X1634_Date_Received__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.RX_MAR_Date_Received__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Reportable Date to end of current quarter</fullName>
        <actions>
            <name>Reportable_Date_field_update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1</booleanFilter>
        <criteriaItems>
            <field>Case.Reportable_to_Health_Canada__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Reportable Health Canada No blanks date</fullName>
        <actions>
            <name>Reportable_date_blanks</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Reportable_to_Health_Canada__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Reportable to Health Canada Yes</fullName>
        <actions>
            <name>Reporting_to_Health_Canada_is_Yes</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Patient_MAiD_Request_Date__c</field>
            <operation>greaterOrEqual</operation>
            <value>11/1/2018</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>notEqual</operation>
            <value>MAiD Assessment of Eligibility,MAiD Request</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Reporting to Health Canada is No</fullName>
        <actions>
            <name>Reporting_to_Health_Canada_is</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Case.Patient_MAiD_Request_Date__c</field>
            <operation>lessThan</operation>
            <value>11/1/2018</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>equals</operation>
            <value>MAiD Assessment of Eligibility,MAiD Request</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
