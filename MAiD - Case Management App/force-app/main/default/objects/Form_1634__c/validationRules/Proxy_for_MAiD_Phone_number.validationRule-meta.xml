<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Proxy_for_MAiD_Phone_number</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Proxy_for_MAiD_Phone_Number__c , &quot;[0-9]{0,10}&quot;) )</errorConditionFormula>
    <errorMessage>Proxy for MAiD Phone number is numeric only</errorMessage>
</ValidationRule>
