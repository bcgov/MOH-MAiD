<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Interpreter_for_MAID_ID_Numeric_Only</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(  Interpreter_for_MAID_ID__c , &quot;[0-9]{0,10}&quot;) )</errorConditionFormula>
    <errorMessage>Interpreter for MAID ID# must be numeric</errorMessage>
</ValidationRule>
