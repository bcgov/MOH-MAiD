<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPSID_BCCNM_Return_Prescriber_Numeric</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(    CPSID_BCCNM_Returning_Prescriber_Licen__c , &quot;[0-9]{0,10}&quot;) )</errorConditionFormula>
    <errorMessage>CPSID / BCCNM Return Prescriber Licence # should be Numeric Only</errorMessage>
</ValidationRule>
